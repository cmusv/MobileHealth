# baseURI: http://www.cmusv.edu/MobileHealth/medicalReadings
# imports: http://cmusv/1.0/11073-Subset/Subset1.0
# imports: http://cmusv/hsl
# imports: http://topbraid.org/sparqlmotionfunctions
# imports: http://topbraid.org/sparqlmotionlib-tbc
# imports: http://www.cmusv.edu/MobileHealth/1.0/Mapping

@prefix afn:     <http://jena.hpl.hp.com/ARQ/function#> .
@prefix arg:     <http://spinrdf.org/arg#> .
@prefix fn:      <http://www.w3.org/2005/xpath-functions#> .
@prefix medicalReadings:  <http://www.cmusv.edu/MobileHealth/medicalReadings#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sm:      <http://topbraid.org/sparqlmotion#> .
@prefix sml:     <http://topbraid.org/sparqlmotionlib#> .
@prefix sp:      <http://spinrdf.org/sp#> .
@prefix spif:    <http://spinrdf.org/spif#> .
@prefix spin:    <http://spinrdf.org/spin#> .
@prefix spl:     <http://spinrdf.org/spl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

sp:diastolicPressure
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:location
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:meanArterialPressure
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:patientId
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:pulseRate
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:readingId
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:systolicPressure
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

sp:units
      rdf:type rdf:Property ;
      rdfs:subPropertyOf sp:arg .

<http://www.cmusv.edu/MobileHealth/medicalReadings>
      rdf:type owl:Ontology ;
      owl:imports <http://cmusv/1.0/11073-Subset/Subset1.0> , <http://www.cmusv.edu/MobileHealth/1.0/Mapping> , <http://topbraid.org/sparqlmotionlib-tbc> , <http://topbraid.org/sparqlmotionfunctions> , <http://cmusv/hsl> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .

medicalReadings:ApplyConstruct_1
      rdf:type sml:ApplyConstruct ;
      sm:next medicalReadings:ExportToDataRDFFile ;
      sml:constructQuery
              [ rdf:type sp:Construct ;
                sp:templates ([ sp:object <http://cmusv/hsl#Patient> ;
                            sp:predicate rdf:type ;
                            sp:subject
                                    [ sp:varName "newHslPatient"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "patientId"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#patientId> ;
                            sp:subject
                                    [ sp:varName "newHslPatient"^^xsd:string
                                    ]
                          ] [ sp:object <http://cmusv/hsl#BPReading> ;
                            sp:predicate rdf:type ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "readingId"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#readingID> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "newPatient"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#ofPatient> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "diastolicPressure"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#diastolicPressure> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "meanArterialPressure"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#meanArterialPressure> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "systolicPressure"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#systolicPressure> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "pulseRate"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#pulseRate> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "units"^^xsd:string
                                    ] ;
                            sp:predicate <http://cmusv/hsl#units> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "currentDate"^^xsd:string
                                    ] ;
                            sp:predicate <http://schema.org/endDate> ;
                            sp:subject
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ]) ;
                sp:where ([ rdf:type sp:Bind ;
                            sp:expression "http://cmusv/hsl#" ;
                            sp:variable
                                    [ sp:varName "hslUri"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type spif:buildURI ;
                                      sp:arg1 [ rdf:type fn:concat ;
                                                sp:arg1 [ sp:varName "hslUri"^^xsd:string
                                                        ] ;
                                                sp:arg2 [ sp:varName "patientId"^^xsd:string
                                                        ]
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "newHslPatient"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type spif:buildURI ;
                                      sp:arg1 [ rdf:type fn:concat ;
                                                sp:arg1 [ sp:varName "hslUri"^^xsd:string
                                                        ] ;
                                                sp:arg2 [ sp:varName "readingId"^^xsd:string
                                                        ]
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "newHslBPReading"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type spif:cast ;
                                      arg:datatype xsd:date ;
                                      sp:arg1 [ rdf:type spif:dateFormat ;
                                                arg:date
                                                        [ rdf:type afn:now
                                                        ] ;
                                                arg:pattern "yyyy-MM-dd"
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "currentDate"^^xsd:string
                                    ]
                          ])
              ] ;
      sml:replace "true"^^xsd:boolean .

medicalReadings:ApplyTopSPIN_1
      rdf:type sml:ApplyTopSPIN ;
      sm:next medicalReadings:ExportToRDFFile_1 ;
      sml:predicate spin:rule ;
      sml:replace "true"^^xsd:boolean .

medicalReadings:ExportToDataRDFFile
      rdf:type sml:ExportToRDFFile ;
      sm:next medicalReadings:ApplyTopSPIN_1 ;
      sml:baseURI "http://www.cmusv.edu/MobileHealth/medicalReadings/data"^^xsd:string ;
      sml:targetFilePath "data.ttl"^^xsd:string .

medicalReadings:ExportToRDFFile_1
      rdf:type sml:ExportToRDFFile ;
      sm:next medicalReadings:translateBloodPressureReadingAsXML ;
      sml:baseURI "http://www.cmusv.edu/MobileHealth/medicalReadings/output"^^xsd:string ;
      sml:targetFilePath "output.rdf"^^xsd:string .

<http://www.cmusv.edu/MobileHealth/medicalReadings#Import-HSL.ttl_1>
      rdf:type sml:ImportRDFFromWorkspace ;
      rdfs:label "Import HSL.ttl"^^xsd:string ;
      sm:next medicalReadings:ApplyConstruct_1 ;
      sml:baseURI "http://cmusv/hsl"^^xsd:string .

<http://www.cmusv.edu/MobileHealth/medicalReadings#Import-Mapping.ttl_1>
      rdf:type sml:ImportRDFFromWorkspace ;
      rdfs:label "Import Mapping.ttl"^^xsd:string ;
      sm:next medicalReadings:ApplyTopSPIN_1 ;
      sml:baseURI "http://www.cmusv.edu/MobileHealth/1.0/Mapping"^^xsd:string .

<http://www.cmusv.edu/MobileHealth/medicalReadings#Import-Subset1.0.ttl_1>
      rdf:type sml:ImportRDFFromWorkspace ;
      rdfs:label "Import Subset1.0.ttl"^^xsd:string ;
      sm:next medicalReadings:ApplyTopSPIN_1 ;
      sml:baseURI "http://cmusv/1.0/11073-Subset/Subset1.0"^^xsd:string .

medicalReadings:translateBloodPressureReadingAsXML
      rdf:type sm:Function , sml:ReturnSPARQLResults ;
      rdfs:subClassOf sm:Functions ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue "Patient528"^^xsd:string ;
                spl:predicate sp:patientId ;
                spl:valueType xsd:string ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue -1 ;
                spl:predicate sp:systolicPressure ;
                spl:valueType xsd:integer ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue -1 ;
                spl:predicate sp:meanArterialPressure ;
                spl:valueType xsd:integer ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue -1 ;
                spl:predicate sp:pulseRate ;
                spl:valueType xsd:integer ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue "BPReading_2"^^xsd:string ;
                spl:predicate sp:readingId ;
                spl:valueType xsd:string ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue -1 ;
                spl:predicate sp:diastolicPressure ;
                spl:valueType xsd:integer ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:defaultValue "mmhg"^^xsd:string ;
                spl:predicate sp:units ;
                spl:valueType xsd:string ;
                sm:next medicalReadings:ApplyConstruct_1
              ] ;
      sm:returnModule medicalReadings:translateBloodPressureReadingAsXML ;
      sml:mimeType "text/xml"^^xsd:string ;
      sml:selectQuery
              [ rdf:type sp:Select ;
                sp:where ([ rdf:type sp:Bind ;
                            sp:expression "http://cmusv/1.0/11073-Subset/Subset1.0#" ;
                            sp:variable
                                    [ sp:varName "subsetUri"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type spif:buildURI ;
                                      sp:arg1 [ rdf:type fn:concat ;
                                                sp:arg1 [ sp:varName "subsetUri"^^xsd:string
                                                        ] ;
                                                sp:arg2 [ sp:varName "patientId"^^xsd:string
                                                        ]
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "newSubsetPatient"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type spif:buildURI ;
                                      sp:arg1 [ rdf:type fn:concat ;
                                                sp:arg1 [ sp:varName "subsetUri"^^xsd:string
                                                        ] ;
                                                sp:arg2 [ sp:varName "readingId"^^xsd:string
                                                        ]
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "newSubsetBPReading"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "o"^^xsd:string
                                    ] ;
                            sp:predicate
                                    [ sp:varName "p"^^xsd:string
                                    ] ;
                            sp:subject
                                    [ sp:varName "s"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "o"^^xsd:string
                                    ] ;
                            sp:predicate
                                    [ sp:varName "p"^^xsd:string
                                    ] ;
                            sp:subject
                                    [ sp:varName "newSubsetBPReading"^^xsd:string
                                    ]
                          ])
              ] ;
      sml:serialization sm:XML .
